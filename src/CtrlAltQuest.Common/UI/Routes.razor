@using Microsoft.AspNetCore.Components.Authorization

@rendermode InteractiveServer
<CascadingAuthenticationState>
    <Router AppAssembly="typeof(Routes).Assembly" AdditionalAssemblies="RoutingAssemblies.Assemblies" @rendermode="InteractiveServer">
        <Found Context="routeData">
            <RouteView RouteData="routeData" DefaultLayout="typeof(CtrlAltQuest.Common.UI.Layouts.MainLayout)" />
        </Found>
    </Router>
</CascadingAuthenticationState>